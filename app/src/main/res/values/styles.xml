<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="Theme.K6nele" parent="Theme.AppCompat">
        <item name="colorAccent">#FFA000</item>
    </style>

    <style name="Theme.K6nele.NoActionBar" parent="Theme.AppCompat.NoActionBar" />

    <style name="Theme.K6nele.Dialog" parent="Theme.AppCompat.Dialog" />

    <style name="Theme.K6nele.Dialog.NoActionBar" parent="Theme.AppCompat.Dialog">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.K6nele.DialogWhenLarge" parent="Theme.AppCompat.DialogWhenLarge" />

    <style name="Theme.K6nele.DialogWhenLarge.NoActionBar" parent="Theme.AppCompat.DialogWhenLarge" />

    <style name="Theme.K6nele.Light" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">#FFC400</item>
        <item name="colorPrimaryDark">#FFA000</item>
        <item name="colorAccent">#FFA000</item>
    </style>

    <style name="Theme.K6nele.Translucent" parent="Theme.AppCompat">
        <item name="windowNoTitle">true</item>
        <item name="windowActionBar">false</item>
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:colorBackgroundCacheHint">@null</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowAnimationStyle">@android:style/Animation</item>
    </style>
</resources>